<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brainbox.student">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".global.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.DashboardActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.PDFDisplayActivity"
            android:label="@string/title_activity_pdfdisplay"></activity>

        <activity android:name=".activities.PoliciesActivity" />
    </application>

</manifest>
